
<%- include('../partials/header.ejs') %> 

<div class="row d-flex justify-contnet-center" >
    <% if(result){%>
        <% if((result.length) ==0){%>
                                              
        <div class="alert alert-danger">
            <a href="#" class="close" data-dismiss="alert" aria-label="close">
                &times;
            </a>
           <span>No Category  Job post Yet</span>
             
         </div>
            <% }else{ %>
       <% for(let i=0;i<result.length; i++) { %>
    <div class="col-xl-3 col-lg-3 col-md-4 col-sm-4 p-5" >
        <div class="single-services text-center mb-20" style="border: 1px solid #ddd; border-radius: 10px">
            <div class="company-img">
                <a href="/companypostjob/>" ><img src="images/daru.png " alt="companylogo" style=" padding:2px;width: 80px;"></a>
            </div>
            <div class="services-cap">
               <h6><a href="/findjobcategory/<%= result[i]._id%>"> <%= result[i]._id %></a></h6>
              
            </div>
            <span>(<%= result[i].count %> )</span>
        </div>
    </div>
   <% } %>
<% } %>
<% } %>
            
          

   
